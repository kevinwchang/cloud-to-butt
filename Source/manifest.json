{
  "manifest_version": 2,
  "name": "Cloud To Butt",
  "version": "1.0",
  "description": "Replaces the text 'the cloud' with 'my butt'.",
  "content_scripts": 
  [
    {
      "matches": ["*://*/*"],
      "js": ["replacements.js", "content_script.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}


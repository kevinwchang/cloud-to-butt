{
  "manifest_version": 2,
  "name": "NFL to Pokémon",
  "version": "1.0.2",
  "description": "Replaces football players' names with names of Pokémon.",
  "content_scripts": 
  [
    {
      "matches": ["*://*/*"],
      "js": ["replacements.js", "content_script.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}

